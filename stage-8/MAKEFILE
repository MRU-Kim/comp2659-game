visuals = objects.o raster.o font.o render.o
gameLogic = model.o events.o
sound = psg.o music.o effects.o
misc = clock.o input.o splash.o dino.o
DEPENDNACIES =  $(visuals) $(gameLogic) $(sound) $(misc)
tstSplDEPEND = tstSpl.o splash.o raster.o objects.o font.o input.o
all: tstSpl
tstSpl: $(tstSplDEPEND)
	cc68x  -g $(tstSplDEPEND) -o tstSpl

tstSpl.o: tstSpl.c
	cc68x -g -c tstSpl.c

splash.o: splash.c
	cc68x -g -c splash.c

objects.o: objects.c
	cc68x -g -c objects.c

raster.o: raster.c
	cc68x -g -c raster.c

font.o: font.c
	cc68x -g -c font.c

render.o: render.c
	cc68x -g -c render.c

input.o: input.c
	cc68x -g -c input.c

rast_asm.o: rast_asm.s
	gen -D -L2 rast_asm.s